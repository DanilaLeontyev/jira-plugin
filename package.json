{
    "name": "jira-plugin",
    "displayName": "jira-plugin",
    "description": "jira plugin",
    "version": "0.0.1",
    "publisher": "jira-plugin",
    "engines": {
        "vscode": "^1.24.0"
    },
    "categories": [
        "Other"
    ],
    "activationEvents": [
        "onCommand:extension.sayHello"
    ],
    "main": "./out/extension",
    "contributes": {
        "commands": [
            {
                "command": "extension.sayHello",
                "title": "Hello World"
            }
        ]
    },
    "scripts": {
        "vscode:prepublish": "npm run compile",
        "compile": "tsc -p ./",
        "watch": "tsc -watch -p ./",
        "postinstall": "node ./node_modules/vscode/bin/install",
        "test": "npm run compile && node ./node_modules/vscode/bin/test"
    },
    "devDependencies": {
        "typescript": "^2.6.1",
        "vscode": "^1.1.6",
        "tslint": "^5.8.0",
        "@types/node": "^7.0.43",
        "@types/mocha": "^2.2.42"
    },
    "repository": {
      "type": "git",
      "url": "git+https://github.com/gioboa/vscode-jira-plugin.git"
    },
    "keywords": [
      "Jira",
      "vsCode",
    ],
    "author": "gioboa",
    "license": "MIT",
    "bugs": {
      "url": "https://github.com/gioboa/vscode-jira-plugin/issues"
    },
    "homepage": "https://github.com/gioboa/vscode-jira-plugin#readme",
}
